function twoSum(nums: number[], target: number): number[] {
    let correctNumbers: number[] = []
        for (let i: number = 0; i < nums.length; i++) {
            for (let j: number = 0; j < nums.length; j++) {
                if(i == j) break;
                if(nums[i] + nums[j] == target) {
                    return correctNumbers = [i, j];
                }
            }
        }
    return [];
};